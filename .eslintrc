{
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "strict": [
            "error",
            "never"
        ],
        "valid-typeof": "error",
        "no-unused-vars": "error",
        "no-unsafe-negation": "error",
        "no-unexpected-multiline": "error",
        "no-undef": "error"
    },
    "env": {
        "browser": true
    }
}
